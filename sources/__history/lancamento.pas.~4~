unit lancamento;


interface
uses
inclusaoLancamento,
principal,
login_view;

implementation

procedure incluirLancamento();
begin
login_view.login.FDConnection1.Connected := True;
login_view.login.FDQuery1.Close;
login_view.login.FDQuery1.SQL.Clear;
login_view.login.FDQuery1.SQL.Add();

end;

end.
